{
    "manifest_version": 2,
    "name": "speedup",
    "version": "0.2",
    "description": "speedup or slowdown the video.Switch the web page to night mode.",
    "icons": {
        "48": "source/icons/积木.svg"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.bilibili.com/video/*"
            ],
            "js": [
                "speedup.js"
            ]
        }
    ]
}